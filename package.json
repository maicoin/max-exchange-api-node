{
  "name": "max-exchange-api-node",
  "version": "3.0.1",
  "description": "Node.js client library for MAX Exchange API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest src",
    "build": "tsup ./src/index.ts --minify",
    "dev": "tsup ./src/index.ts --watch",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "docs": "typedoc",
    "test:coverage": "vitest run --coverage",
    "release": "npm run lint && npm test && standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maicoin/max-exchange-api-node.git"
  },
  "keywords": [
    "maicoin",
    "max",
    "exchange",
    "api",
    "client",
    "nodejs"
  ],
  "author": "max@maicoin.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maicoin/max-exchange-api-node/issues"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitest/coverage-v8": "2.1.2",
    "bintrees": "^1.0.2",
    "change-case": "^5.4.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "tsup": "^8.2.4",
    "typedoc": "^0.26.7",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.6.3",
    "vitest": "^2.1.2",
    "zod": "^3.23.8"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  }
}
