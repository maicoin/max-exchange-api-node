{
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "entryPoints": [
        "src/index.ts",  
        "src/types.ts",  
        "src/rest/types.ts",
        "src/rest/index.ts",
        "src/rest/wallet.ts",
        "src/rest/schema.ts",
        "src/ws/types.ts",
        "src/ws/index.ts",
        "src/ws/schema.ts"
    ],
    "out": "./docs",
    "disableSources": true,
    "exclude": [
        "**/*.spec.ts",
        "**/*.test.ts",
        "**/node_modules/**",
        "**/events.d.ts",
        "**/eventemitter.d.ts"
    ]
}